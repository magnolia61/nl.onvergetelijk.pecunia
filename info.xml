<?xml version="1.0"?>
<extension key="nl.onvergetelijk.pecunia" type="module">
  <file>pecunia</file>
  <name>Onvergetelijke Zomerkampen - Pecunia</name>
  <description>Deze extensie zorgt dat het te declareren bedrag altijd met een negatieve waarde wordt vastgelegd.</description>
  <license>GNU AGPL</license>
  <maintainer>
    <author>Richard van Oosterhout</author>
    <email>webteam@onvergetelijk.nl</email>
  </maintainer>
  <releaseDate>2022-08-01</releaseDate>
  <version>1.0</version>
  <develStage>stable</develStage>
  <compatibility>
    <ver>5.45</ver>
  </compatibility>
  <comments>Stichting Onvergetelijke Zomerkampen</comments>
  <civix>
    <namespace>CRM/Pecunia</namespace>
    <format>23.02.1</format>
  </civix>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <mixins>
    <mixin>menu-xml@1.0.0</mixin>
    <mixin>mgd-php@1.0.0</mixin>
  </mixins>
</extension>
